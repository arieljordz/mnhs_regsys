@{
    ViewBag.Title = "ONLINE REGISTRATION";
    Layout = "~/Views/Shared/_LayOut.cshtml";
}
<div class="content-wrapper pt-5 mt-2" style="min-height: 767.986px; max-width: 100%;">
    <div class="box-body" style="background-color: #daefdf;">
        <section class="content-header">
       <div class="card">
    <div class="card-header bg-primarys">
        <h3 class="card-title">
            ENCODED DATA FORM
        </h3>
    </div>
    <div class="card-body" style="padding: 0px !important;">
        <div class="bgLightGreen BorderTop pb-3 pr-3 pl-3 pt-1 mt-4 ml-4 mr-4">
            <div class="row">
                <label>
                    DETAILS</label>
            </div>
            <form id="frmEncodedData" novalidate class="form-horizontal" data-parsley-required-message="">
            <div class="row mb-1">
                <div class="col-md-2">
                    No of Teachers
                    <input type="number" class="form-control" name="NoOfTeacher" required="required"/>
                </div>
                <div class="col-md-2">
                    No of Class Rooms
                    <input type="number" class="form-control" name="NoOfClassRoom" required="required"/>
                </div>
                <div class="col-md-2">
                    No of Computers
                    <input type="number" class="form-control" name="NoOfComputer" required="required"/>
                </div>
                <div class="col-md-2">
                    No of Toilets
                    <input type="number" class="form-control" name="NoOfComfortRoom" required="required"/>
                </div>
                   <div class="col-md-2">
                    No of Seats
                    <input type="number" class="form-control" name="NoOfSeat"  required="required"/>
                </div>
            </div>
            </form>
        </div>
        <div class="d-flex justify-content-end mt-3 mb-3 pr-4 pl-4">
        <button class="btn btn-primary hide" type="submit" id="btnUpdateEncodedData">
                Update Data</button>
            <button class="btn btn-primary ml-3" type="submit" id="btnSaveEncodedData">
                Save Data</button>
        </div>
        <div class="d-flex p-2 bg-primarys text-white">
            <div class="pl-2" style="font-size: 1.25rem; font-weight: 400; margin: 0;">
                LIST OF ENCODED DATA</div>
        </div>
          <div class="bgLightGreen BorderTop pb-3 pr-3 pl-3 pt-1 m-4">
            <div class="row p-4">
                <div class="col-md-12">
                    <div class="dataTable_wrapper table-responsive">
                        <table id="EncodedDataTable" class="table table-bordered text-center" style="width: 100%">
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

        </section>
    </div>
</div>
@section scripts{
    <script src="@Url.Content("~/Scripts/JSFileMaintenance.js")" type="text/javascript"></script>
    @*<script src="@Url.Content("~/Scripts/JSDashboard.js")" type="text/javascript"></script>*@
    <script type="text/javascript">
        ActiveMenu("#Adviser");
    </script>
}
